{
  "expo": {
    "name": "Mote",
    "slug": "mote-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mote.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/mote-ios.png",
      "backgroundColor": "#33002A",
      "supportsTablet": true,
      "bundleIdentifier": "org.mote.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#9B5C8D"
      },
      "package": "org.mote.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mote.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#9B5C8D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
